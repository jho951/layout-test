  <!-- footer 나타내기 --> 
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      gsap.registerPlugin(ScrollTrigger);
      gsap.fromTo("#footer",
      { opacity:0 },
      {opacity:1, duration: 1,
        scrollTrigger: {
          trigger: ".back-image", 
          start: "bottom bottom",
          end: "bottom+=200 bottom",
          scrub: true,
          toggleActions: "play none none none",
        },
      });
    });
  </script>
  <!-- back 이미지 확대 --> 
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      gsap.registerPlugin(ScrollTrigger);
      gsap.fromTo(".back-image", 
        {scale: 1},
        { scale: 1.5,
        scrollTrigger: {
          trigger: ".back",  // back 영역을 트리거로 설정
          start: "top 60%",  // back 영역이 화면에 60% 오면 애니메이션 시작
          end: "top 20%",  // back 영역이 화면에 20% 남았을 때 애니메이션 종료
          scrub: true,  // 스크롤과 애니메이션 동기화
          toggleActions: "play none none none",  // 완료 후 고정
        } 
      });
    });
  </script>
  <!-- title 글자 차례대로 나타남 --> 
  <script>
    document.addEventListener('DOMContentLoaded', function () {
    gsap.registerPlugin(ScrollTrigger);
    gsap.fromTo('.title', 
      {opacity: 0}, 
      {opacity: 1, duration: 1, stagger: 1.5,
        scrollTrigger: {
        trigger: '.title-text',
        start: "top 100%", 
        end: "top 60%",
        scrub: true,
        toggleActions: "play none none none",
      }});
    });
  </script>

<script>
document.addEventListener("DOMContentLoaded", function() {
    const marquee = document.querySelector('.marquee');
    const container = document.querySelector('.marquee-container');
    
    let position = 0;  // 애니메이션의 위치
    const speed = 0.1;  // 속도 (속도를 조절하려면 이 값을 변경)
    let containerWidth = container.offsetWidth;  // 부모 컨테이너의 초기 크기

    // 화면 크기에 맞춰서 translate3d의 이동 범위 계산
    function calculateMaxMoveDistance() {
        const totalWidth = marquee.offsetWidth;  // 마퀴가 차지하는 전체 너비
        return totalWidth - containerWidth;  // 이동할 수 있는 최대 거리
    }

    // 애니메이션 함수
    function animateMarquee() {
        // 현재 위치를 점차적으로 왼쪽으로 이동
        position -= speed;

        // 화면 크기 변화에 맞춰 최대 이동 범위 갱신
        const maxMoveDistance = calculateMaxMoveDistance();

        // 위치가 최대 이동 범위를 넘어가면 다시 처음으로 리셋
        if (position <= -maxMoveDistance) {
            position = 0;
        }

        // `transform: translate3d()`를 실시간으로 위치 변경
        marquee.style.transform = `translate3d(${position}px, 0, 0)`;

        // 다음 애니메이션 프레임 요청
        requestAnimationFrame(animateMarquee);
    }

    // 화면 크기 변화에 대응
    window.addEventListener('resize', function() {
        const newContainerWidth = container.offsetWidth;
        
        // 컨테이너 크기가 변경된 경우, position 초기화 후 다시 애니메이션 시작
        if (newContainerWidth !== containerWidth) {
            containerWidth = newContainerWidth;
            position = 0;  // 크기 변경 시 애니메이션 초기화
        }
    });

    // 애니메이션 시작
    animateMarquee();
});
</script>
<script>
  $(document).ready(function() {
    var waypoint = new Waypoint({
      element: document.getElementById('count'),
      handler: function() {
        var count = 0;
        // data-count_total 속성 값 읽기
        var target = 1000
        
        var interval = setInterval(function() {
          count++;
          $('#count').text(count);
          if (count >= target) {
            clearInterval(interval);
          }
        }, 1);
      },
      offset: '75%' // 요소가 화면에 75% 보일 때 실행
    });
  });
</script>
</s_t3>
</body>
</html>